<AvailableService>:
    BoxLayout:
        orientation: 'vertical'

        SliverToolbar:
            id: silver_tool_bar
            title: "Available Services"
            left_action_items: [["arrow-left", lambda x: root.go_back()]]
            elevation: 10

        ScrollView:
            MDBoxLayout:
                id: content
                orientation: "vertical"
                padding: "12dp"
                spacing: "12dp"
                adaptive_height: True
