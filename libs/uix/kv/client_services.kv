<CustomImageTile>:
    radius: 24
    box_radius: [0, 0, 24, 24]
    box_color: 1, 1, 1, .2
    source: "cats.jpg"
    pos_hint: {"center_x": .5, "center_y": .5}
    size_hint: None, None
    size: "320dp", "150dp"
    MDIconButton:
        icon: "heart-outline"
        theme_icon_color: "Custom"
        icon_color: 1, 0, 0, 1
        pos_hint: {"center_y": .5}
        on_release: self.icon = "heart" if self.icon == "heart-outline" else "heart-outline"

    MDLabel:
        id:label
        text: "Julia and Julie"
        bold: True
        color: 1, 1, 1, 1

<ClickableTextFieldRound>:





<HeroItem>
    id:hero
    size_hint_y: None
    size_hint_x: None
    height: '120dp'
    width: '100dp'
    radius: 10
    MDSmartTile:
        id: tile
        radius: 10
        box_radius: 0, 0, 10, 10
        box_color: 0, 0, 0, 0.5
        source: root.text
        size_hint: None, None
        size: root.size
        mipmap: True
        on_release: root.on_release()

        MDLabel:
            text: root.text2
            bold: True
            font_style: "Subtitle2"
            halign:'center'
            opposite_colors: True

